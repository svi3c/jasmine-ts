{
  "name": "@ert78gb/jasmine-ts",
  "version": "0.3.1",
  "description": "Execute jasmine with ts-node",
  "main": "lib/index.js",
  "bin": "lib/index.js",
  "scripts": {
    "precompile": "rimraf lib",
    "compile": "tsc --project tsconfig.json",
    "lint": "tslint -p tsconfig.json",
    "jasmineTs": "node lib/index.js --config=spec/jasmine.json spec/jasmine-ts.spec.ts --transpileOnly --compilerOptions='{\"allowJs\": true}'",
    "test": "npm run lint && npm run compile && npm run jasmineTs",
    "prepublishOnly": "npm run compile",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ert78gb/jasmine-ts.git"
  },
  "keywords": [
    "node",
    "testing",
    "tests",
    "jasmine",
    "typescript"
  ],
  "author": "Sven Reglitzki <sreglitzki@gmail.com>",
  "maintainers": [
    "RÃ³bert Kiss <ert78gb@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ert78gb/jasmine-ts/issues"
  },
  "homepage": "https://github.com/ert78gb/jasmine-ts#readme",
  "files": [
    "lib"
  ],
  "dependencies": {
    "yargs": "^16.2.0"
  },
  "peerDependencies": {
    "jasmine": ">=3.4",
    "ts-node": ">=3.2.0 <=10",
    "typescript": ">=3.5.2"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.3",
    "@types/node": "^14.14.25",
    "@types/yargs": "^16.0.0",
    "jasmine": "^3.6.3",
    "jasmine-spec-reporter": "^6.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.0.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  }
}
